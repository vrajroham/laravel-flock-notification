{
    "name": "vrajroham/laravel-flock-notification",
    "description": "Laravel Flock Notification Channel",
    "homepage": "https://github.com/vrajroham/laravel-flock-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Vaibhavraj Roham",
            "email": "vaibhavraj.developer@gmail.com",
            "homepage": "https://vrajroham.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4 || >=8.0 || >=8.1",
        "illuminate/notifications": ">=7",
        "illuminate/support": ">=7"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": ">=7"
    },
    "autoload": {
        "psr-4": {
            "Vrajroham\\LaravelFlockNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vrajroham\\LaravelFlockNotification\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vrajroham\\LaravelFlockNotification\\FlockServiceProvider"
            ]
        }
    }
}
